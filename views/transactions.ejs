<%-include("Partials/header")-%>
<div class="container ftco-section">
  <div class="row wizard-v6-content">
    <div class="col-lg-8 offset-lg-2">
      <div class="card wizard-form">
        <div class="card-header wizard-heading">
          <h1>All Transactions</h1>
        </div>
        <div class="card-body  .wizard-for form-register out but">
          <ul class="list-group list-group-flush inner">
            <% if(a.length > 0) { %>
            <% for(let c of a) {%>
            <li class="list-group-item form-select form-control wizard-form inner" style="text-transform: capitalize;">
              <%= c.payment %> <b>----- <%= c.Date %></b>
            </li>
            <% } %>
            <% } else { %>
            <li class="list-group-item form-select form-control inner wizard-form" style="text-transform: capitalize;">
              Sorry!!!! No transactions Yet........
            </li>
            <% } %>
          </ul>
        </div>
      </div>
    </div>
  </div>
</div>
<%-include("Partials/footer")-%>
